{
  "name": "optimistdigital/nova-settings",
  "description": "A Laravel Nova tool for editing custom settings using native Nova fields.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaSettings\\": "src/"
    },
    "files": [
      "./src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaSettings\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
