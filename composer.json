{
    "name": "optimist-digital/nova-settings",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "OptimistDigital\\NovaSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OptimistDigital\\NovaSettings\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
